{
  "name": "nexcess-ksplice",
  "version": "0.1.1",
  "author": "nexcess",
  "summary": "Install and configure ksplice for Centos, RHEL, Fedora, Debian, and Ubuntu",
  "license": "Apache-2.0",
  "source": "https://github.com/nexcess/puppet-ksplice",
  "project_page": "https://github.com/nexcess/puppet-ksplice",
  "issues_url": "https://github.com/nexcess/puppet-ksplice/issues",
  "tags": [
    "kernel",
    "ksplice"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "15.04"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [
        "21",
        "22"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">= 2.0.0"
    }
  ]
}
